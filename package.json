{
  "name": "utilities-v2",
  "version": "0.0.1",
  "description": "",
  "author": "hung3a8",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env FORCE_COLOR=1 turbo run build",
    "dev": "cross-env FORCE_COLOR=1 turbo run dev --parallel",
    "dev:main": "pnpm dev --filter core --filter user",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/core/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/core/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@types/ip": "^1.1.3",
    "@types/multer": "^1.4.11",
    "argon2": "^0.31.2",
    "class-transformer": "^0.5.1",
    "cron": "^3.1.3",
    "formdata-node": "^6.0.3",
    "ip": "^1.1.8",
    "mongoose": "^8.0.4",
    "mysql2": "^3.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "ping": "^0.4.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.19",
    "video.js": "^8.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.11.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/ping": "^0.4.4",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-svelte": "2.36.0-next.4",
    "eslint-plugin-tailwindcss": "3.14.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "3.1.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^1.11.3",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}