#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/utilities-v2/node_modules/.pnpm/vite@3.2.10_@types+node@18.19.33_sass@1.77.2_terser@5.26.0/node_modules/vite/bin/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vite@3.2.10_@types+node@18.19.33_sass@1.77.2_terser@5.26.0/node_modules/vite/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vite@3.2.10_@types+node@18.19.33_sass@1.77.2_terser@5.26.0/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/utilities-v2/node_modules/.pnpm/vite@3.2.10_@types+node@18.19.33_sass@1.77.2_terser@5.26.0/node_modules/vite/bin/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vite@3.2.10_@types+node@18.19.33_sass@1.77.2_terser@5.26.0/node_modules/vite/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vite@3.2.10_@types+node@18.19.33_sass@1.77.2_terser@5.26.0/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
