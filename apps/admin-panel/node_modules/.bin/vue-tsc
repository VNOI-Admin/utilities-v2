#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/utilities-v2/node_modules/.pnpm/vue-tsc@0.38.9_typescript@5.4.5/node_modules/vue-tsc/bin/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vue-tsc@0.38.9_typescript@5.4.5/node_modules/vue-tsc/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vue-tsc@0.38.9_typescript@5.4.5/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/utilities-v2/node_modules/.pnpm/vue-tsc@0.38.9_typescript@5.4.5/node_modules/vue-tsc/bin/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vue-tsc@0.38.9_typescript@5.4.5/node_modules/vue-tsc/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/vue-tsc@0.38.9_typescript@5.4.5/node_modules:/home/ubuntu/utilities-v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
fi
